local w, h = love.graphics.getWidth(), love.graphics.getHeight()
local museum
local cancel = function()
end
audioHandler = audioHandler or require"audioHandler"
museum = {
    {
        name = "appleGuy",
        x = w * .3375,
        y = h / 3,
        w = w * .34375,
        h = h * .36666666,
        imagePath = "assets/museum_apple_guy.png",
        type = sprite
    },
    {
        name = "Museum",
        w = w,
        h = h,
        imagePath = "assets/Museum.png",
        type = sprite
    },
    onShow = function()
        cancel = audioHandler.loop"museum"
    end,
    onClear = function()
        cancel() --Don't simplify this to "onClear = cancel", the lazy evaluation is what makes this work.
    end
}
return museum