local w, h = love.graphics.getWidth(), love.graphics.getHeight()
local museum
audioHandler = audioHandler or require"audioHandler"
museum = {
    {
        name = "appleGuy",
        x = w * .3375,
        y = h / 3,
        w = w * .34375,
        h = h * .36666666,
        imagePath = "assets/museum_apple_guy.png",
        type = sprite
    },
    {
        name = "Museum",
        w = w,
        h = h,
        imagePath = "assets/Museum.png",
        type = sprite
    },
    onShow = function()
        museum.onCancel = audioHandler.loop"museum"
    end,
}
return museum